<container class="searchfield">
  <form onsubmit="doSearch(event, this)">
    <label>Search:
      <input type="text" id="search-input" name="searchText" value="San"/>
    </label>
    <button type="submit">Search</button>
  </form>

  <div id="result-count"></div>
  <table id="result-table"></table>
</container>

<button type="button" id="viewVehicles" name="button">Click here to view list of vehicles</button>

<div class="vehiclelist" id="vehiclelist">
  <h1>Vehicles</h1>
</div>

<%= javascript_include_tag "vehicles" %>